# Copy this file to terraform.tfvars and fill in your values
# DO NOT commit terraform.tfvars to git (it's in .gitignore)
# IMPORTANT: Use LIVE/PRODUCTION keys for this environment!

aws_region  = "eu-west-1"
environment = "prod"

# Get from https://console.anthropic.com/settings/keys
claude_api_key = "sk-ant-api03-..."

# Get from https://dashboard.stripe.com/apikeys (use LIVE keys for production!)
stripe_secret_key = "sk_live_..."

# Get from https://dashboard.stripe.com/webhooks
stripe_webhook_secret = "whsec_..."

# Update with your actual frontend URLs
cognito_callback_urls = [
  "https://koumpa.com/auth/callback",
  "https://www.koumpa.com/auth/callback"
]

cognito_logout_urls = [
  "https://koumpa.com",
  "https://www.koumpa.com"
]
